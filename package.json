{
  "name": "discord-drafter",
  "version": "0.1.0",
  "private": true,
  "description": "Discord draft bot with state stored inside the draft message",
  "license": "MIT",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
